                              </HBox>
                              <HBox alignment="TOP_CENTER" prefHeight="94.0" prefWidth="272.0">
                                 <children>
                                    <VBox>
                                       <children>
                                          <Button fx:id="AddCompetitorButton" mnemonicParsing="false" onAction="#AddCompetitor" text="Add Competitor" />
