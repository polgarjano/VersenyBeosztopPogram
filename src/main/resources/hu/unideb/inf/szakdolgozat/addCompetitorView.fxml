                                    <Insets top="40.0" />
                                 </padding>
                              </Label>
                              <HBox alignment="BOTTOM_LEFT" prefHeight="348.0" prefWidth="110.0">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#Back" prefHeight="60.0" prefWidth="80.0" text="Back" />
                                 </children>
